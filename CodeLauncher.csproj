<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <BaseOutputPath>H:\ProgramFiles\CodeLauncher\</BaseOutputPath>
    <ApplicationIcon>vscode.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="vscode.ico" />
  </ItemGroup>

</Project>
